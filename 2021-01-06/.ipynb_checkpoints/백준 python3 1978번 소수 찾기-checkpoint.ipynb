    "a=int(input())\n",
    "arr=[]\n",
    "arr[0:]=map(int,input().split())\n",
    "k=0\n",
    "for x in arr:\n",
    "    if x==1:\n",
    "        k+=0\n",
    "        continue\n",
    "    elif x==2:\n",
    "        k+=1\n",
    "    else:\n",
    "        for y in range(2,x):\n",
    "            if x%y==0:\n",
    "                break\n",
    "            elif x==y+1:\n",
    "                k+=1\n",
    "print(k)"
   ]
