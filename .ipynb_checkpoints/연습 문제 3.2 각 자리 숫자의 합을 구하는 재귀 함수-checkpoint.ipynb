    "\n",
    "def sumOfDigits(x):\n",
    "    a=int(x%10)\n",
    "    b=int(x//10)\n",
    "    if x>0:\n",
    "        return a+sumOfDigits(b)\n",
    "    else:\n",
    "        return 0\n",
    "    \n",
    "        \n",
    "\n",
    "i=int(input(\"숫자:\"))\n",
    "print(sumOfDigits(i))"
